{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/vue3-essential",
        "@nuxt/eslint-config"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "parser":"babel-eslint"
    },
    "rules":{
        "strict":0
    },
    "plugins": [
        "vue"
    ]
}
